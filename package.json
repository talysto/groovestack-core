{
  "name": "core",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-old": "npx prettier -c **/*.{tsx,ts,json} && pnpm --filter core-base-fe build && pnpm --filter core-comments-fe build && pnpm --filter core-versions-fe build && pnpm --filter core-jobs-fe build && pnpm --filter core-accounting-fe build && pnpm --filter core-webhooks-fe build",
    "build": "pnpm -r --workspace-concurrency=-1 --filter './core*' build",
    "build-except-jobs": "pnpm -r --workspace-concurrency=-1 --filter './core*' --filter '!./core-jobs-fe' build",
    "lint": "eslint --report-unused-disable-directives --max-warnings 0 ./core-base-fe",
    "lint:vscode": "eslint --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --format vscode .",
    "dev": "pnpm --filter test-core-fe dev",
    "format": "npx prettier -w **/*.{tsx,ts,json}",
    "pre-publish": "pnpm publish -r --dry-run --no-git-checks",
    "publish-all": "pnpm publish -r",
    "beta:docs": "npx @microsoft/api-documenter markdown -i core-base-fe/docs -o docs/docs/api/",
    "storybook": "storybook dev -p 6006",
    "storybook:test": "test-storybook --coverage && nyc report --reporter=text  -t coverage/storybook",
    "storybook:build": "storybook build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "build:watch": "npx onchange -f change '**/*' -e '**/dist/**' -e 'docs/**' -e '**/stats.html' -- echo {{event}} {{changed}}",
    "watch": "find $PKG/src | entr pnpm -F $PKG build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "@chromatic-com/storybook": "^1.6.1",
    "@eslint/js": "^9.6.0",
    "@faker-js/faker": "^8.4.1",
    "@mui/icons-material": ">=5.14.1",
    "@mui/material": ">=5.14.1",
    "@storybook/addon-coverage": "^1.0.4",
    "@storybook/addon-essentials": "^8.2.1",
    "@storybook/addon-interactions": "^8.2.1",
    "@storybook/addon-links": "^8.2.1",
    "@storybook/addon-onboarding": "^8.2.1",
    "@storybook/blocks": "^8.2.1",
    "@storybook/react": "^8.2.1",
    "@storybook/react-vite": "^8.2.1",
    "@storybook/test": "^8.2.1",
    "@storybook/test-runner": "^0.19.0",
    "@types/eslint__js": "^8.42.3",
    "@types/lodash": "^4.17.6",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.6.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.8",
    "eslint-plugin-storybook": ">=0.8.0",
    "globals": "^15.8.0",
    "lodash": "^4.17.21",
    "nyc": "^17.0.0",
    "prettier": "^3.3.2",
    "prettier-plugin-organize-imports": "^4.0.0",
    "ra-data-fakerest": "^5.0.4",
    "react": "^18.0",
    "react-admin": "^5.0",
    "react-dom": "^18.0",
    "rollup-plugin-auto-external": ">=2.0.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "storybook": "^8.2.1",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.0",
