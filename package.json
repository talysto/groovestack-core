{
  "name": "core",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-old": "npx prettier -c **/*.{tsx,ts,json} && pnpm --filter core-base-fe build && pnpm --filter core-comments-fe build && pnpm --filter core-versions-fe build && pnpm --filter core-jobs-fe build && pnpm --filter core-accounting-fe build && pnpm --filter core-webhooks-fe build",
    "build": "pnpm -r --workspace-concurrency=-1 --filter './core*' build",
    "lint": "eslint --ext ts,tsx --report-unused-disable-directives --max-warnings 0 .",
    "lint:vscode": "eslint --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --format vscode .",
    "dev": "pnpm --filter test-core-fe dev",
    "format": "npx prettier -w **/*.{tsx,ts,json}",
    "pre-publish": "pnpm publish -r --dry-run --no-git-checks",
    "publish-all": "pnpm publish -r",
    "beta:docs": "npx @microsoft/api-documenter markdown -i core-base-fe/docs -o docs/docs/api/",
    "storybook": "storybook dev -p 6006",
    "storybook:test": "test-storybook --coverage && nyc report --reporter=text  -t coverage/storybook",
    "storybook:build": "storybook build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "build:watch": "npx onchange -f change '**/*' -e '**/dist/**' -e 'docs/**' -e '**/stats.html' -- echo {{event}} {{changed}}",
    "watch": "find $PKG/src | entr pnpm -F $PKG build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
